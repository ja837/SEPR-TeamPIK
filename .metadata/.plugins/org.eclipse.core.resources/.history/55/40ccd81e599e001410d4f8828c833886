
package com.teampik.game;

import com.badlogic.gdx.graphics.g2d.TextureRegion;
import com.badlogic.gdx.maps.tiled.tiles.StaticTiledMapTile;
import com.badlogic.gdx.math.Vector2;

//This class is for handling the tile which the item is on
public class itemtile extends TrackTile {
	
	Vector2 coords;
	int type;
	
	public itemtile(TextureRegion textureRegion, powerups params) {
		
		super(textureRegion, params.coordinates);
		
		this.coords = params.coordinates;
		this.type = params.type;
		
	}
}

